{
  "name": "firex-store",
  "version": "1.4.0",
  "description": "Subscribe and write firestore data, easily",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "test": "jest -w 1 --forceExit",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nor-ko-hi-jp/firex-store.git"
  },
  "keywords": [
    "vuex",
    "firebase",
    "firestore",
    "vue",
    "nuxt"
  ],
  "author": "nor-ko-hi-jp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nor-ko-hi-jp/firex-store/issues"
  },
  "homepage": "https://github.com/nor-ko-hi-jp/firex-store#readme",
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-typescript": "^7.7.7",
    "@types/jest": "^24.0.24",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "firebase": "^7.6.1",
    "flush-promises": "^1.0.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "files": [
    "lib/**/*",
    "docs/**/*"
  ]
}
